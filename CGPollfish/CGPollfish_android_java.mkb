#!/usr/bin/env mkb
# Builder mkb file for the java portion of the CGPollfish extension on android
platform JAVA

files
{
    (source/android)
    CGPollfish.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"

librarypath "thirdParty/pollfish.jar"
option output-name=lib/android/CGPollfish.jar
